<div class = "container">

  <nav class="navbar navbar-default">
    <div class="container-fluid">
      <div class="navbar-header">
        <a class="navbar-brand" href="#">ActivityTracker</a>
        <%= link_to "Add user", new_user_path %>
      </div>
    </div>
  </nav>


  <% @users.each do |u| %>
