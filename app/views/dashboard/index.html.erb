<div class = "container">

  <nav class="navbar navbar-default">
    <div class="container-fluid">
      <div class="navbar-header">
        <a class="navbar-brand" href="#">ActivityTracker</a>
      </div>
      <%= link_to "Add user", new_user_path, class: "btn btn-default"%>
    </div>
  </nav>


  <% @users.each do |u| %>
    <p>
      <%= u.name %>
      <% u.exercises.each do |e| %>
       <a href =
        "https://github.com/<%= u.name %>/<%=u.repo %>/releases/<%= e.name %>"
        class = "exercise-success">
        <%= e.name %>
      </a>
      <% end %>
    </p>
  <% end %>

</div>
